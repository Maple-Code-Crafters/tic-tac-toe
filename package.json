{
  "name": "tic-tac-toe",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "ionic serve",
    "build": "tsc && ionic build",
    "build:android": "ionic capacitor copy android",
    "build:ios": "ionic capacitor copy ios",
    "test.e2e": "cypress run",
    "test.e2e:debug": "cypress run --headed --no-exit",
    "test.unit": "vitest",
    "test.unit:debug": "vitest --run --logHeapUsage --bail 1",
    "lint": "npx eslint . --fix",
    "p": "npm run lint && npx prettier --log-level=warn --write .",
    "prepare": "husky",
    "generate": "npx @capacitor/assets generate --ios --android --iconBackgroundColor '#ffffff' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#ffffff' --splashBackgroundColorDark '#111111'"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/preferences": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic/react": "^8.0.1",
    "@ionic/react-router": "^8.0.1",
    "@reduxjs/toolkit": "^2.2.3",
    "@sentry/capacitor": "^0.17.0",
    "@sentry/react": "7.93.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "ionicons": "^7.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.1",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^6.0.0",
    "@eslint/js": "^9.1.1",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.8.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "terser": "^5.31.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.10",
    "vitest": "^1.5.2"
  },
  "description": "An Ionic project",
  "engines": {
    "node": "22",
    "npm": "10"
  }
}
